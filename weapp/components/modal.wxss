.model .bg {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  opacity: 0.7;
  z-index: 999;
  background-color: #000;
}
.model .container {
  background-color: #fff;
  z-index: 1000;
  position: fixed;
  width: 286px;
  height: auto;
  left: 50%;
  top: 50%;
  border-radius: 5px;
  padding: 0 10px;
  transform: translate3d(-50%, -50%, 0);
}
.model .container .title {
  height: 46px;
  line-height: 46px;
  text-align: center;
  font-size: 18px;
  color: #333;
  border-bottom: 1px solid #f5f5f5;
}
.model .container .content {
  padding: 20px 20px;
}
.model .container .btns {
  height: 46px;
  border-top: 1px solid #f5f5f5;
  line-height: 46px;
  display: flex;
}
.model .container .btns .btn {
  flex: 1;
  font-size: 15px;
  color: #333;
}
.model .container .btns .btn.left {
  text-align: left;
  margin-left: 20px;
}
.model .container .btns .btn.right {
  color: #FE0000;
  text-align: right;
  margin-right: 20px;
}

